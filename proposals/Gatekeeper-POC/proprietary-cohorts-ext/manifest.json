{
  "name": "identity-gatekeeper-cohorts",
  "version": "0.1",
  "description": "Gatekeeper Extension ",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "privacy",
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*"
    ]
  },
  "manifest_version": 2
}
