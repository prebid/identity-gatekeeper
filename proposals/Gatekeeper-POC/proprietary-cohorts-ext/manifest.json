{
  "name": "identity-gatekeeper-cohorts",
  "version": "0.1",
  "description": "Gatekeeper Extension",
  "browser_action": {
    "default_icon": "magnite.jpg",
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "privacy",
    "storage",
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://de-cohort-endpoint-dev.usw2.fanops.net/*"
    ]
  },
  "manifest_version": 2
}
